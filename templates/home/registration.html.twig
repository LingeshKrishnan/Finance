{% extends ('base.html.twig') %}

{% block title %}
    Registration Page
{% endblock title %}

{% block body%}
    <section class="regform" id="regform">
        <div class="row">

            <div class="col-lg-6 col-md-6 col-sm-12">
                    <div class="container">
                        <h3 align="center">Register Here Manually</h3>
                    </div>
                    <form>
                        <div class="form-group">
                            <label for="name">Name: </label>
                            <input type="text" name="name" id="name" placeholder="Enter your Fullname" required />
                        </div>
                        <div class="form-group">
                            <label for="email">E-mail: </label>
                            <input type="text" name="email" id="email" placeholder="Enter your mail address" required />
                        </div>
                        <div class="form-group">
                            <label for="number">Phone: </label>
                            <input type="text" name="number" id="number" placeholder="Enter your Phone Number" required />
                        </div>
                    </form>
            </div>

             <div class="col-lg-6 col-md-6 col-sm-12">
             
                <div class="container">
                            <h3 align="center">Login Here </h3>
                        </div>
                        <div class="g-signin2" data-onsuccess="onSignIn" data-theme="dark"></div>
                            <script>
                            function onSignIn(googleUser) {
                                var profile = googleUser.getBasicProfile();
                                // Useful data for your client-side scripts:
                                console.log('Full Name: ' + JSON.stringify(profile));
                                document.querySelector('#content').innerText = googleUser.getBasicProfile().getName();
                            }
                            </script>
               
                </div>
           
             
        </div>
    </section>
{% endblock body%}